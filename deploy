#!/bin/bash
# Docker deploy script generated by deploy-make

[ -f ./.deploy-test ] && source ./.deploy-test
[ -f ./.deploy ]     || curl https://raw.githubusercontent.com/nicferrier/docker-shell-deploy/master/deploy-helpers -o ./.deploy     || { echo "can't http the deployscript" ; exit 1; }
. ./.deploy
dockerImage=nicferrier/marmalade
dockerExPort=8005
nginxConfig=/etc/init.d/nginx/sites-enabled/marmalade.conf
hostName=po3.ferrier.me.uk
dockerVolumes=+/home/nferrier/marmalade:/home/emacs/marmalade/
deploy ${1:-"deploy"} nicferrier/marmalade 8005 /etc/init.d/nginx/sites-enabled/marmalade.conf po3.ferrier.me.uk +/home/nferrier/marmalade:/home/emacs/marmalade/
